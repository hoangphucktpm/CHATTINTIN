{
  "expo": {
    "name": "AppTinTin",
    "slug": "AppTinTin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-document-picker",
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow $(AppTinTin) to open the camera",
          "microphonePermission": false
        }
      ],
      "expo-av",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(AppTinTin) to access your camera",
          "microphonePermission": "Allow $(AppTinTin) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.hoangphucktpm.AppTinTin",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2e276bc6-3107-4d90-9398-6f316258559c"
      }
    }
  }
}
